[credential]
	helper = store
[include]
	path = ~/Secrets/.gitconfig
[includeIf "gitdir:~/Works/"]
	path = ~/Works/.gitconfig

[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[core]
	editor = vim
	precomposeunicode = true
	autocrlf = input
	excludesFile = ~/.config/git/ignore
[rerere]
	enabled = true
[pull]
	rebase = true
[init]
	defaultBranch = main
